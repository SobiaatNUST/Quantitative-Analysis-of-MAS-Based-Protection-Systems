// Probability of Isolation Failure for fault in Zone 1
P=? [ F ("Fail1"|"Fail2") ]

// Probability of Auxiliary Algorithm called
P=? [ F ((CTM2=2|CTM1=2|CTM2=3|CTM1=3)|(WD2=1|WD1=1)) ]

// Probability of Isolation Success for fault in Zone 1
P=? [ (F "Cond1"&(F "Succ1"))|(F "Cond2"&(F "Succ2")) ]

// Risk for Case I Zone I
P=? [ ((F "risk1"))|((F "risk2")) ]

