// Probability of Desired Configuration Scenario 
P=? [ F (WD2>0|WD1>0) ]

// Probability of Isolation Success for fault in Zone 1
P=? [ (F "Cond1"&(F "Succ1"))|(F "Cond2"&(F "Succ2")) ]

// Probability of Isolation Failure for fault in Zone 1
P=? [ F ("Fail1"|"Fail2") ]

P=? [ (F "Cond1"&(F "False_trip1"))|(F "Cond2"&(F "False_trip2")) ]

P=? [ F ("False_trip1"|"False_trip2") ]

P=? [ (F "risk") ]

