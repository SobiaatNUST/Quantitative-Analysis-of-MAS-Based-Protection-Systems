// Probability of Isolation Failure for fault in Zone 4
P=? [ F ("Fail1"|"Fail2") ]

// Probability of Desired Configuration Scenario 
P=? [ F (WD3>0|WD2>0) ]

// Probability of Isolation Success for fault in Zone 3
P=? [ (F "Cond1"&(F "Succ1"))|(F "Cond2"&(F "Succ2")) ]

// Probability of System under Risk for fault in Zone 3
P=? [ F "Risk" ]

// Probability of False Tripping for fault in Zone 3
P=? [ F "False_trip" ]

